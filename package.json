{
  "name": "leaflet-material-controls",
  "version": "1.0.0",
  "description": "Leaflet controls inspired by Google's material design and using the material-design-lite framework.",
  "main": "src/leaflet-material.js",
  "browser": {
    "material-control": "./src/material_control/material_control.js"
  },
  "browserify-shim": {
    "leaflet": "global:L"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "dependencies": {
    "@mapbox/mapbox-sdk": "^0.12.1",
    "leaflet-fullscreen": "1.0.2",
    "material-design-lite": "^1.3.0"
  },
  "devDependencies": {
    "rollup": "^2.47.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.13"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/christippett/leaflet-material.git"
  },
  "keywords": [
    "leaflet",
    "material",
    "google",
    "mapbox",
    "control",
    "mdl"
  ],
  "author": "Chris Tippett",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/christippett/leaflet-material/issues"
  },
  "homepage": "https://github.com/christippett/leaflet-material"
}
